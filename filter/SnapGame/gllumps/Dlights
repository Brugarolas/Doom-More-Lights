// Snap Dynamic lights
// (More Lights made by Jakie)
// Basic Gun

pointlight SNAP_ROCKET
{
    color 1.0 0.7 0.0
    size 84
	attenuate 1
}

flickerlight SNAP_ROCKET_X1
{
    color 1.0 0.7 0.5
    size 96
    secondarySize 108
    chance 0.3
	attenuate 1
}

flickerlight SNAP_ROCKET_X2
{
    color 0.5 0.3 0.2
    size 120
    secondarySize 132
    chance 0.3
	attenuate 1
}

flickerlight SNAP_ROCKET_X3
{
    color 0.3 0.1 0.1
    size 144
    secondarySize 156
    chance 0.3
	attenuate 1
}

object SnapBasicShot
{
    frame BULLA { light SNAP_ROCKET    }

    frame BULLB { light SNAP_ROCKET_X1 }
    frame BULLC { light SNAP_ROCKET_X2 }
    frame BULLD { light SNAP_ROCKET_X3 }
	frame BULLE { light SNAP_ROCKET_X3 }
	frame BULLF { light SNAP_ROCKET_X3 }
}

